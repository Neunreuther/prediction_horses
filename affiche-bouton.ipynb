    "import tkinter as tk\n",
    "\n",
    "def action_bouton():\n",
    "    print(\"Bouton cliqué!\")\n",
    "\n",
    "fenetre = tk.Tk()\n",
    "fenetre.title(\"Exemple de bouton\")\n",
    "fenetre.geometry(\"300x200\")\n",
    "\n",
    "bouton = tk.Button(fenetre, text=\"Cliquez ici\", command=action_bouton)\n",
    "bouton.pack(pady=20)\n",
    "\n",
    "fenetre.mainloop()\n"
